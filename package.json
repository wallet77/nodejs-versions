    "coverage": "NODE_ENV=test nyc --reporter=html --reporter=lcovonly --reporter=text mocha ./test/**/*.mocha.js",
    "linter": "eslint ./test ./src"
  },
  "engines": {
    "node": ">=12.0.0"
