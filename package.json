{
  "name": "nodejs-versions",
  "version": "1.0.13",
  "description": "Provides information about all NodeJs version and releases.",
  "private": false,
  "main": "index.js",
  "bin": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    "require": "./index.js",
    "import": "./index.js"
  },
  "files": [
    "src/**",
    "cache/**"
  ],
  "scripts": {
